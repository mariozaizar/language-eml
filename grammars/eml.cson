    { 'match': '^(?i)x-received.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-report-abuse.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-report-abuse.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-sender.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-source-args.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-source-dir.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-source.*?:', 'name': 'constant.other.eml' },
