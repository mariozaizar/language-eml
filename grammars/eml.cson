    { 'match': '^(?i)x-tmn.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-virus-scanned.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-yahoo-newman-id.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-yahoo-newman-property.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-ymail-isg.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)x-ymail-osg.*?:', 'name': 'constant.other.eml' },

    # Specials
    { 'match': '(?i)7bit', 'name': 'markup.raw.eml' },
    { 'match': '(?i)base64', 'name': 'markup.raw.eml' },
    { 'match': '(?i)multipart\/.*:', 'name': 'markup.raw.eml' },
    { 'match': '(?i)text\/.*;', 'name': 'markup.raw.eml' },
    { 'match': '(?i)boundary', 'name': 'markup.raw.eml' },
    { 'match': '^--(?!>).*', 'name': 'keyword.control' },
    { 'match': '^(-)?--(--.*)?', 'name': 'comment.line.eml' },

    # Encoded
    { 'match': '^\s\*.*', 'name': 'comment.line.eml' },
    { 'match': '^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$', 'name': 'comment.line.eml' }

    # WIP text/html
