{
  'scopeName': 'text.eml.basic'
  'name': 'EML (E-Mail MIME Message)'
  'fileTypes': ['eml', 'email', 'mail', 'mime']
  'patterns': [
    # Emails
    { 'match': '"[a-zA-Z0-9. \+\_\-]+"\s*&lt;[a-zA-Z0-9.\-]+\@[a-zA-Z0-9.\-]+&gt;', 'name': 'variable.other.eml' },
    { 'match': '[a-zZ-Z0-9.\+\_\-]*\s*&lt;[a-zA-Z0-9.\-]+\@[a-zA-Z0-9.\-]+&gt;', 'name': 'variable.other.eml' },
    { 'match': '[a-zA-Z0-9.\+\_\-]+\@[a-zA-Z0-9.\-]+', 'name': 'variable.other.eml' },

    # Headers
    { 'match': '^(?i)accept-language.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)authentication-results.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)bcc.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)cc.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)charset.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)comments.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)content-disposition.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)content-id.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)content-language.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)content-transfer-encoding.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)content-type.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)date.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)delivered-to.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)delivery-date.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)dkim-signature.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)domainkey-signature.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)envelope-from.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)enverlope-to.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)from.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)in-reply-to.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)keywors.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)list-help.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)list-id.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)list-post.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)list-unsubscribe.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)message-id.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)mime-version.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)msmail-priority.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)precedence.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)priority.*?:', 'name': 'constant.other.eml' },
    { 'match': '^(?i)received-spf.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)received.*?:', 'name': 'constant.language.eml' }
    { 'match': '^(?i)references.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)reply-to.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)return-path.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)sender.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)sent:.*?', 'name': 'constant.language.eml' },
    { 'match': '^(?i)subject.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)thread-index.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)thread-topic.*?:', 'name': 'constant.language.eml' },
    { 'match': '^(?i)to:.*?', 'name': 'constant.language.eml' },
    { 'match': '^(?i)user-agent.*?:', 'name': 'constant.language.eml' },

    # Resent Headers
    { 'match': '^.*(?i)resent-bcc.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-cc.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-date.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-from.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-message-id.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-reply-to.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-sender.*?:', 'name': 'constant.other.eml' },
    { 'match': '^.*(?i)resent-to.*?:', 'name': 'constant.other.eml' },

